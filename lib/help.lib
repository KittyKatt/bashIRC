#!/bin/bash
function help.show() {
   notice $(nick) "$me""'s help"
   while read help; do
      notice $(nick) "$help"
   done < /tmp/.bashirc/help
   notice $(nick) "$(cat .help.count) help showed."
}
function help.hook() {
   #Hooks onto help
   echo "${@}" >> /tmp/.bashirc/help
   help_count=$(cat .help.count)
   help_count=$((help_count+1))
   echo "$help_count" > .help.count
}
